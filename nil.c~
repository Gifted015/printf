#include "main.h"

/**
 *null - prints the string "(null)"
 *@len: used to add the lenght of "null" to
 *the lenght of string originally printed
 *Return: nothing
 */

void null(__attribute__((unused)) int *len)
{
int x;
char empty[] = "(null)";

for (x = 0; empty[x] != '\0'; x++)
{
_putchar(empty[x]);
*len = (*len) + 1;
}
}
